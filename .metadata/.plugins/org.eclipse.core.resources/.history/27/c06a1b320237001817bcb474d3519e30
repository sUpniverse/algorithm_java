package changeBinary;

import java.util.Scanner;

public class MainClass {
	
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		double num = 0;
		int sum = 0;
		
		for(int i = 0 ; i < 333334; i ++) {
			num = (a % 10) * Math.pow(8, i);
			sum += num;
		}
		System.out.println();
	}

}
