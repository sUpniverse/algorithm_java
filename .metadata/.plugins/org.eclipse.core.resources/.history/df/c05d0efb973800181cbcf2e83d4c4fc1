package taejun;

import java.util.Scanner;

public class taejun {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		
		int a = sc.nextInt();
		int b = sc.nextInt();
		int c = sc.nextInt();
		int answer = 0;
		
		while(c != 0) {
			c -= a;
			c -= b;
			answer++;
		}
		
		System.out.println(answer);
	}
}
